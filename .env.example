# Example environment variables for Terragrunt
# Copy this file to .env and fill in your actual values
# Then run: export $(cat .env | xargs) before running terragrunt

# AWS Configuration
export TF_VAR_aws_region="us-east-1"
export TF_VAR_vpc_id="vpc-xxxxxxxxx"
export TF_VAR_key_name="your-ssh-key-name"

# GitHub Configuration
export TF_VAR_github_token="your-github-token"
export TF_VAR_github_repo="https://github.com/ngelrojas/fast_api_jwt"

# Application Secrets
export TF_VAR_secret_key="your-jwt-secret-key"
export TF_VAR_user_name="your-app-username"
export TF_VAR_password="your-app-password"
export TF_VAR_url_base="http://your-app-url.com"

# Optional: S3 and DynamoDB for state (if different from defaults)
export TF_VAR_bucket_name="tf-state-locks-fast-api-jwt"
export TF_VAR_table_name="tf-table-locks-fast-api-jwt"
